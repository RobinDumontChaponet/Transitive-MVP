{
	"name": "transitive/project",
	"description": "An empty project using Transitive.",
	"type": "project",
	"authors": [
	{
		"name": "Robin Dumont-Chaponet",
		"email": "src@dumontchapo.net",
		"homepage": "https://robin.dumontchapo.net",
		"role": "Developer"
	}],
	"require": {
		"php": "^7.0",
		"transitive/core":"dev-master",
		"transitive/utils":"dev-master"
	},
	"autoload": {
		"classmap": ["install/ScriptHandler.class.php"]
	},
	"scripts": {
		"setup": "Transitive\\Core\\Install\\ScriptHandler::setup",
		"post-create-project-cmd": [
			"@setup",
			"Transitive\\Project\\Install\\ScriptHandler::setup",
			"composer install -o --quiet"
		],
		"post-install-cmd": "@setup"
	}
}